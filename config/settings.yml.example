# -*- coding:utf-8 -*-
development:
  version: "v1.0.0"
  index_map:
    latitude: 38.4344802
    longitude: 141.3029167
  ldap_auth:
    enable: false
  external_auth:
    enable: true
    google_enable: true
    twitter_enable: true
    facebook_enable: true
  # SMTPサーバの設定
  mail:
    address: smtp.example.com
    port: 587
    authentication: :plain
    user_name: username
    password: password
    domain: smtp.example.com
    enable_starttls_auto: true
    sender: saitai@example.com
    host: localhost:3000
  # ログイン画面のアカウント新規登録項目の表示・非表示
  # * true : 表示
  # * false: 非表示
  account_display_enable: true
